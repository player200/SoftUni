@model  IEnumerable<SuppliersModel>

@{
    ViewData["Title"] = "Local";
}

<h2>Local</h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Parts</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var supplier in Model)
        {
            <tr>
                <td>@supplier.Id</td>
                <td>@supplier.Name</td>
                <td>@supplier.Parts</td>
            </tr>
        }
    </tbody>
</table>