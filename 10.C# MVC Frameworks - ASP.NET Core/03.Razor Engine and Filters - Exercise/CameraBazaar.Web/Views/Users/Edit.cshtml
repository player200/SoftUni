@model CreateUserModel
@{
    ViewData["Title"] = "Edit";
}
<h2>Edit</h2>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="Email"></label>
                <input asp-for="Email" class="form-control" />
                <span asp-validation-for="Email" class="form-text text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Phone"></label>
                <input asp-for="Phone" class="form-control" />
                <span asp-validation-for="Phone" class="form-text text-danger"></span>
            </div>
            <div>
                <a class="btn btn-default" asp-area="" asp-controller="Cameras" asp-action="All">Cancel</a>
                <input class="btn btn-success" type="submit" name="Edit User" />
            </div>
            <p> Last time login:@Model.LastTimeLoged</p>
        </form>
    </div>
</div>
